    allowNull: false
  },
  email: {
    type: Sequelize.STRING,
    unique: true,
    allowNull: false
  },
  photoURL: {
    type: Sequelize.STRING
  },
  uid: {
    type: Sequelize.STRING,
typeUserId    primaryKey: true,
    allowNull: false,
    unique: true,
  },
  typeUserId: {
	type: Sequelize.INTEGER,
	},
};

const User = db.define('users', fields
);

module.exports = {
  Model:User,
  fields
};


